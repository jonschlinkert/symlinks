{
  "name": "symlinks",
  "description": "Glob symlinks, async or sync. API and CLI. CLI also has `--del` for deleting matching symlinks.",
  "version": "0.1.1",
  "homepage": "https://github.com/jonschlinkert/symlinks",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/symlinks",
  "bugs": {
    "url": "https://github.com/jonschlinkert/symlinks/issues"
  },
  "license": "MIT",
  "files": [
    "cli.js",
    "index.js"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "symlinks": "cli.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "ansi-red": "^0.1.1",
    "extend-shallow": "^2.0.1",
    "is-symlink": "^0.1.1",
    "matched": "^0.4.1",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "keywords": [
    "async",
    "fs",
    "link",
    "lstat",
    "symbolic",
    "symboliclink",
    "symlink",
    "sync"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "matched",
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
